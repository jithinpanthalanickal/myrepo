<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee">
    <display-name>SERVLETS</display-name>
       <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.jithin.code.Login</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/signin</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Signup</servlet-name>
        <servlet-class>com.jithin.code.CandidateSignUp</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Signup</servlet-name>
        <url-pattern>/signup</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ResetCheck</servlet-name>
        <servlet-class>com.jithin.code.Reset1</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ResetCheck</servlet-name>
        <url-pattern>/resetcheck</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ResetPassword</servlet-name>
        <servlet-class>com.jithin.code.Reset2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ResetPassword</servlet-name>
        <url-pattern>/reset</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Quiz</servlet-name>
        <servlet-class>com.jithin.code.Quiz</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Quiz</servlet-name>
        <url-pattern>/quizit</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Submission</servlet-name>
        <servlet-class>com.jithin.code.Submit</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Submission</servlet-name>
        <url-pattern>/submit</url-pattern>
    </servlet-mapping>
 </web-app>

